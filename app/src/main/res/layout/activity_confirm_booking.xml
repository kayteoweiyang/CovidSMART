<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ConfirmBooking">

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="349dp"
        android:layout_height="45dp"
        android:layout_marginTop="36dp"
        android:background="#FF9696"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/titile" />

    <TextView
        android:id="@+id/LocConfirm"
        android:layout_width="138dp"
        android:layout_height="0dp"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/aldrich"
        android:shadowColor="@color/black"
        android:text="Location : "
        android:textAlignment="textStart"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/TimeConfirm"
        app:layout_constraintTop_toBottomOf="@+id/TimeConfirm" />

    <TextView
        android:id="@+id/txtLocConfirm"
        android:layout_width="180dp"
        android:layout_height="0dp"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/aldrich"
        android:hint="Location"
        android:shadowColor="@color/black"
        android:textAlignment="textStart"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/txtTimeConfirm"
        app:layout_constraintTop_toBottomOf="@+id/txtTimeConfirm" />

    <Button
        android:id="@+id/backConfirm"
        android:layout_width="150dp"
        android:layout_height="60dp"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/aldrich"
        android:text="Back"
        app:layout_constraintEnd_toEndOf="@+id/bookConfirm"
        app:layout_constraintStart_toStartOf="@+id/bookConfirm"
        app:layout_constraintTop_toBottomOf="@+id/bookConfirm" />

    <Button
        android:id="@+id/bookConfirm"
        android:layout_width="190dp"
        android:layout_height="61dp"
        android:layout_marginTop="252dp"
        android:fontFamily="@font/aldrich"
        android:text="Confirm and Back To Homepage"
        app:layout_constraintEnd_toEndOf="@+id/headerConfirm"
        app:layout_constraintStart_toStartOf="@+id/headerConfirm"
        app:layout_constraintTop_toBottomOf="@+id/headerConfirm" />

    <TextView
        android:id="@+id/txtTimeConfirm"
        android:layout_width="180dp"
        android:layout_height="0dp"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/aldrich"
        android:hint="Time"
        android:shadowColor="@color/black"
        android:textAlignment="textStart"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/txtDateConfirm"
        app:layout_constraintTop_toBottomOf="@+id/txtDateConfirm" />

    <TextView
        android:id="@+id/TimeConfirm"
        android:layout_width="138dp"
        android:layout_height="0dp"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/aldrich"
        android:shadowColor="@color/black"
        android:text="Time : "
        android:textAlignment="textStart"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/DateConfirm"
        app:layout_constraintTop_toBottomOf="@+id/DateConfirm" />

    <TextView
        android:id="@+id/txtDateConfirm"
        android:layout_width="180dp"
        android:layout_height="0dp"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/aldrich"
        android:hint="Date"
        android:shadowColor="@color/black"
        android:textAlignment="textStart"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/txtTypeConfirm"
        app:layout_constraintTop_toBottomOf="@+id/txtTypeConfirm" />

    <TextView
        android:id="@+id/DateConfirm"
        android:layout_width="138dp"
        android:layout_height="0dp"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/aldrich"
        android:shadowColor="@color/black"
        android:text="Date : "
        android:textAlignment="textStart"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/typeConfirm"
        app:layout_constraintTop_toBottomOf="@+id/typeConfirm" />

    <TextView
        android:id="@+id/txtTypeConfirm"
        android:layout_width="180dp"
        android:layout_height="0dp"
        android:layout_marginTop="44dp"
        android:fontFamily="@font/aldrich"
        android:hint="Type"
        android:shadowColor="@color/black"
        android:textAlignment="textStart"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/headerConfirm"
        app:layout_constraintTop_toBottomOf="@+id/headerConfirm" />

    <TextView
        android:id="@+id/typeConfirm"
        android:layout_width="138dp"
        android:layout_height="0dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="44dp"
        android:fontFamily="@font/aldrich"
        android:shadowColor="@color/black"
        android:text="Type : "
        android:textAlignment="textStart"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@+id/headerConfirm"
        app:layout_constraintStart_toStartOf="@+id/headerConfirm"
        app:layout_constraintTop_toBottomOf="@+id/headerConfirm" />

    <TextView
        android:id="@+id/headerConfirm"
        android:layout_width="339dp"
        android:layout_height="28dp"
        android:layout_marginTop="15dp"
        android:text="Booking Confirmation"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/imageView7" />

</androidx.constraintlayout.widget.ConstraintLayout>